<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- 如果想在5.0以前版本使用Material Design设计，那么必须使用support:appcompat-v7 ，AppCompatActivity，和Theme.AppCompat主题

     -->
    <style name="AppThemeCompatV7" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="colorControlNormal">@color/colorControlNormal</item>
        <item name="colorControlActivated">@color/colorControlActivated</item>
        <item name="colorSwitchThumbNormal">@color/colorSwitchThumbNormal</item>
    </style>


    <!-- Theme.Holo主题适用于api 11 ~higher 平台-->
    <style name="AppThemeV11" parent="android:Theme.Holo.Light.NoActionBar">
    </style>

    <array name="button_raise_color">
        <item>@style/LightRaiseColorButtonRippleStyle</item>
        <item>@style/DarkRaiseColorButtonRippleStyle</item>
    </array>

    <array name="button_raise_wave_color">
        <item>@style/LightRaiseWaveColorButtonRippleStyle</item>
        <item>@style/DarkRaiseWaveColorButtonRippleStyle</item>
    </array>

    <array name="edittext">
        <item>@style/LightEditText</item>
        <item>@style/DarkEditText</item>
    </array>

    <array name="pv_circular">
        <item>@style/LightCircularProgressView</item>
        <!--<item>@style/DarkCircularProgressView</item>-->
    </array>
</resources>